@{
}
<table class="table table-bordered">
    <tr class="text-center">
        <th>Id</th>
        <th>Name</th>
        <th>City</th>
        <th>State</th>
        <th>Salary</th>
    </tr>
    @foreach (var d in Model)
    {
        <tr class="text-center">
            <td>@d.Id</td>
            <td>@d.Name</td>
            <td>@d.City</td>
            <td>@d.State</td>
            <td>@d.Salary</td>
        </tr>
    }
</table>
<button style="background-color: #007BFF; color: #fff; padding: 10px 20px; border: none; border-radius: 5px; cursor: pointer; font-size: 16px;">
    <a asp-controller="EmpData" asp-action="Back" style="text-decoration: none; color: #fff;">Back</a>
</button>

